<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.id </td>
      <td> article.title  article.text  link_to 'Show', article_path(article) </td>
    </tr>
   end 
</table>
